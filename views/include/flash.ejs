<% if (success && success.length) { %>
    <div class="alert alert-success alert-dismissible fade show col-6   mt-2 offset-3" role="alert">
        <%= success %>
        <button type="submit" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>
<% } %>



<% if (fail && fail.length) { %>
  <div class="alert alert-warning alert-dismissible fade show col-6   mt-2 offset-3" role="alert">
      <%= fail %>
      <button type="submit" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<% } %>





<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js"></script>